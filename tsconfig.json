{
  // re-grab matt pocock tsconfig cheat sheet
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "~/convex/*": ["./convex/_generated/*"],
      "~/new-common": ["./common/index.ts"]
    }
  }
  // "include": ["src", "@types", "public/theme.js"]
}
